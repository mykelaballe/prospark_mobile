{
  "name": "ProSpark",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "a": "react-native run-android",
    "i": "react-native run-ios",
    "g": "cd android && gradlew assembleRelease --console plain",
    "c": "cd android && gradlew clean",
    "ca": "cd android && gradlew clean && cd .. && react-native run-android",
    "pad": "code-push release-react ProSpark-User-Android android --deploymentName Development -t *",
    "pid": "code-push release-react ProSpark-User-iOS ios --deploymentName Development -t *",
    "pap": "code-push release-react ProSpark-User-Android android --deploymentName Production",
    "pip": "code-push release-react ProSpark-User-iOS ios --deploymentName Production",
    "e": "cd android/app/build/outputs/apk && explorer .",
    "adb": "adb kill-server && adb start-server",
    "adbdev": "adb -s 50a79b4a reverse tcp:8081 tcp:8081"
  },
